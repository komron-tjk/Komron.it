{
    "version": 2,
    "builds": [
        {
            "src": "api/telegram.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/telegram",
            "dest": "/api/telegram.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}